<?xml version="1.0"?>
<launch>

  <!-- Joy Node -->
  <node respawn="true" pkg="joy" type="joy_node" name="turtle_joy" >
      <param name="dev" type="string" value="/dev/input/js0" />
      <param name="deadzone" value="0.01" />
      <param name="autorepeat_rate" value="60" />
  </node>

  <!-- ROSSerial Python -->
  <node name="serial_node" pkg="rosserial_python" type="serial_node.py">
      <param name="port" value="/dev/ttyACM0"/>
      <param name="baud" value="57600" />
  </node>

  <!-- ZED -->
  <!--- <include file="$(find zed_wrapper)/launch/zed.launch" /> -->

  <!-- Traxxas Control -->
  <include file="$(find traxxas_control)/launch/joy_control.launch" /> 


  <!-- RealSense Camera -->
  <include file="$(find realsense2_camera)/launch/rs_aligned_depth_MD2.launch" />

</launch>
